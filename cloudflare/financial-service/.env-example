# Version
VERSION=unknown

# Database and Storage
# These are typically provided by Cloudflare Workers, so no need to set them manually
# DB=
# KV=
# STORAGE=
# BANK_STATEMENTS=
# TELLER_CERT=
# USER_ACTIONS_QUEUE=

# API and Secret Keys
API_SECRET_KEY=your_api_secret_key
GOCARDLESS_SECRET_ID=your_gocardless_secret_id
GOCARDLESS_SECRET_KEY=your_gocardless_secret_key
PLAID_CLIENT_ID=your_plaid_client_id
PLAID_SECRET=your_plaid_secret
PLAID_ENVIRONMENT=sandbox_or_development_or_production

# Typesense Configuration
TYPESENSE_API_KEY=your_typesense_api_key
TYPESENSE_ENDPOINT=your_typesense_endpoint
TYPESENSE_ENDPOINT_AU=your_typesense_endpoint_au
TYPESENSE_ENDPOINT_EU=your_typesense_endpoint_eu
TYPESENSE_ENDPOINT_US=your_typesense_endpoint_us

# Upstash Redis Configuration
UPSTASH_REDIS_REST_TOKEN=your_upstash_redis_rest_token
UPSTASH_REDIS_REST_URL=your_upstash_redis_rest_url

# Stripe Configuration
STRIPE_SECRET_KEY=your_stripe_secret_key

# Unkey Configuration
UNKEY_API_KEY=your_unkey_api_key

# Cloudflare Configuration (Optional)
CLOUDFLARE_API_KEY=your_cloudflare_api_key
CLOUDFLARE_ZONE_ID=your_cloudflare_zone_id

# Environment Configuration
ENVIRONMENT=development
PLATFORM_PREFIX=solomonai_platform

# Additional variables from the original .env-example
SUPABASE_KEY=your_supabase_key
SUPABASE_URL=your_supabase_url
