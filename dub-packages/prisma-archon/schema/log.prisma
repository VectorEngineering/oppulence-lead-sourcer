// Log references endpointId -> endpoints.id
model Log {
  id         String     @id @default(cuid())
  endpointId String
  type       LogType?
  postType   LogPostType?
  message    Json
  createdAt  DateTime

  // RELATION
  endpoint   Endpoint   @relation(fields: [endpointId], references: [id])

  @@index([endpointId])
}
