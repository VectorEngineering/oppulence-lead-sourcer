{
  "name": "@playbookmedia/thinkthank",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"pnpm prisma:generate && next dev --turbo --port 8888\" \"pnpm prisma:generate && pnpm prisma:studio --browser none\" \"email dev --port 3333\"",
    "build": "pnpm clean:prisma && pnpm prisma:generate && next build",
    "lint": "next lint",
    "start": "next start",
    "script": "tsx ./scripts/run.ts",
    "test": "vitest -no-file-parallelism",
    "generate-openapi": "tsx ./scripts/generate-openapi.ts",
    "prisma:generate": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma-thinkthank generate",
    "prisma:push": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma-thinkthank push",
    "prisma:studio": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma-thinkthank studio",
    "clean": "rm -rf node_modules && rm -rf .turbo && rm -rf .next",
    "jobs": "dotenv-flow -e .env -- pnpm prisma:generate && pnpm dlx trigger.dev@latest dev",
    "jobs:deploy": "dotenv-flow -e .env -- pnpm dlx trigger.dev@latest deploy",
    "clean:prisma": "rm -rf node_modules/.prisma",
    "postinstall": "pnpm prisma:generate",
    "rebuild": "pnpm clean && pnpm install && pnpm build"
  },
