{
  "name": "@playbookmedia/archon",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently --kill-others \"pnpm prisma:generate && next dev --turbo --port 5555\" \"pnpm prisma:generate && pnpm prisma:studio --browser none\"",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down -v",
    "docker:start": "pnpm docker:up && sleep 5 && pnpm docker-dev",
    "docker:clean": "./scripts/clean-docker.sh",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "precommit": "pnpm format && pnpm build",
    "build": "pnpm prisma:generate && next build",
    "start": "next start",
    "test:unit": "vitest",
    "db:generate": "drizzle-kit generate:pg",
    "db:migrate": "tsx lib/db/migrate.ts",
    "db:studio": "drizzle-kit studio",
    "generate": "drizzle-kit generate",
    "lint": "biome lint",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "ngrok": "ngrok http http://localhost:3000",
    "db:migrate:local": "NODE_ENV=development POSTGRES_URL=\"postgresql://postgres:mypassword@localhost:5434/postgres\" npm run db:migrate",
    "db:dump": "tsx scripts/dump-users.ts",
    "clean": "rm -rf .next && rm -rf node_modules",
    "prisma:generate": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma-archon generate",
    "prisma:push": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma-archon push",
    "prisma:studio": "dotenv-flow -e .env -- pnpm --filter=@dub/prisma-archon studio"
  },
